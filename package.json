{
  "name": "subrisa-site",
  "version": "1.0.0",
  "description": "Subrisa.com website made with react and prismic.io",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subrisa/subrisa-site.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/subrisa/subrisa-site/issues"
  },
  "homepage": "https://github.com/subrisa/subrisa-site#readme",
  "dependencies": {
    "next": "^5.1.0",
    "nprogress": "^0.2.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "recompose": "^0.26.0"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^3.1.1",
    "now": "^11.1.7"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "deploy": "now -e NODE_ENV=production --token $NOW_TOKEN --npm --public -T brisa",
    "alias": "now alias --token=$NOW_TOKEN -T brisa"
  },
  "now": {
    "name": "subrisa-com",
    "alias": "subrisa-com"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./"
          ]
        }
      ]
    ]
  }
}
